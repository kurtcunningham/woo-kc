{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"title": "Carousel",
	"name": "gridible/static-carousel-container",
	"description": "Showcase images, text, or multimedia in a sleek, swipe-friendly slider, perfect for highlighting galleries, featured content, or promotions.",
	"category": "design",
	"allowedBlocks": [
		"gridible/static-carousel-slide"
	],
	"attributes": {
		"carouselType": {
			"type": "string",
			"default": "static"
		}
	},
	"providesContext": {
		"gridible/carouselType": "carouselType"
	},
	"supports": {
		"anchor": true,
		"align": true,
		"ariaLabel": false,
		"className": true,
		"color": {
			"background": true,
			"gradients": true,
			"link": false,
			"text": false
		},
		"customClassName": true,
		"dimensions": {
			"minHeight": false
		},
		"layout": true,
		"multiple": true,
		"reusable": true,
		"position": false,
    "spacing": {
			"margin": true,
			"padding": true,
			"blockGap": false
    },
		"typography": false,
		"interactivity": true
	},
	"editorScript": "gridible-blocks/editor/script",
	"editorStyle": "gridible-blocks/editor/style",
	"style": "gridible-blocks/front/style"
}