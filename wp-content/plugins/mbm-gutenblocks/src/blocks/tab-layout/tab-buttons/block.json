{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"title": "Tab Buttons",
	"description": "Enabling you to incorporate and customize individual tab buttons, offering versatility and creativity in structuring your content.",
	"name": "mbm/tab-buttons",
	"category": "design",
	"ancestor": [
		"mbm/tab-layout"
	],
	"attributes": {
		"buttonsOrientation": {
			"type": "string",
			"default": "row"
		},
		"rowButtonsOverflow": {
			"type": "string",
			"default": "wrap"
		}
	},
	"supports": {
		"anchor": true,
		"align": true,
		"ariaLabel": true,
		"className": true,
		"color": {
			"background": true,
			"gradients": false,
			"link": true,
			"text": true
		},
		"customClassName": true,
		"layout": {
			"default": {
				"type": "flex"
			}
		},
		"multiple": true,
		"reusable": true,
		"spacing": {
			"margin": true,
			"padding": true,
			"blockGap": true,
			"__experimentalDefaultControls": {
				"padding": true,
				"blockGap": true
			}
		},
		"__experimentalBorder": {
			"color": true,
			"radius": false,
			"style": true,
			"width": true,
			"__experimentalDefaultControls": {
				"color": true,
				"radius": false,
				"style": true,
				"width": true
			}
		},
		"typography": {
			"fontSize": true,
			"lineHeight": true,
			"__experimentalFontFamily": true,
			"__experimentalFontWeight": true,
			"__experimentalFontStyle": true,
			"__experimentalTextTransform": true,
			"__experimentalTextDecoration": true,
			"__experimentalLetterSpacing": true,
			"__experimentalDefaultControls": {
				"fontSize": true
			}
		},
		"interactivity": true
	},
	"amplify": {
		"viewNamespace": "mbm/tabLayout/tabButton"
	},
	"editorScript": "mbm-gutenblocks/editor/script",
	"editorStyle": "mbm-gutenblocks/editor/style",
	"style": "mbm-gutenblocks/front/style"
}